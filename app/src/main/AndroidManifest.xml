<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smallbizinventory">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Inventory List"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OrderHistoryActivity"
            android:exported="false"
            android:label="Order History"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
                android:name=".ListsActivity"
                android:exported="false"
                android:label="Order History"
                android:theme="@style/AppTheme">
            <meta-data
                    android:name="android.app.lib_name"
                    android:value="" />
        </activity>
        <activity
                android:name=".InventoryActivity"
                android:exported="false"
                android:label="Login"
                android:theme="@style/AppTheme">
            <meta-data
                    android:name="android.app.lib_name"
                    android:value="" />
        </activity>
        <activity
                android:name=".EditItemsActivity"
                android:exported="false"
                android:label="Login"
                android:theme="@style/AppTheme" />
            <meta-data
                    android:name="android.app.lib_name"
                    android:value="" />
        <activity
                android:name=".ReOrderItemsActivity"
                android:exported="false"
                android:label="Reorder List"
                android:theme="@style/AppTheme">
            <meta-data
                    android:name="android.app.lib_name"
                    android:value="" />
        </activity>
        <activity
                android:name=".NewUserActivity"
                android:exported="false"
                android:label="New User"
                android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                    android:name="android.app.lib_name"
                    android:value="" />
        </activity>
        </application>


</manifest>